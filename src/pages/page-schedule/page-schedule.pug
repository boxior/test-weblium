    .page-schedule.schedule.container.state-0-all
        header(role="presentation").schedule__header.header
            h1(role="presentation").header__title.Block-title Schedule
            h3(role="presentation").header__subtitle.Subtitle Apart from working on effective products for businesses, our specialists organize educational programs for business owners and managers.
        main.schedule__main.main
            header(role="presentation").main__header
                ul(role="directory").main__heading.heading
                    li(role="presentation" aria-label="Day of educational program").heading__item.Heading-large Date
                    li(role="presentation" aria-label="Time of educational program").heading__item.Heading-large Time
                    li(role="presentation" aria-label="Class of educational program").heading__item.Heading-large Class
                    li(role="presentation" aria-label="Coach of educational program").heading__item.Heading-large.coach Coach
                    li(role="presentation" aria-label="Level of educational program").heading__item.Heading-large.level Level
            article(role="main").main__section.section
                article(role="main").section__item
                    h2(role="presentation" aria-label="Monday").section__day.Heading Monday
                        i.arrow-down
                    .section__desc.desc(role="contentinfo" )
                        each item in dayM
                            +days(item)
                article(role="main").section__item
                    h2(role="presentation" aria-label="Tuesday").section__day.Heading Tuesday
                        i.arrow-down
                    .section__desc.desc(role="contentinfo")
                        each item in dayT
                            +days(item)
                article(role="main").section__item
                    h2(role="presentation" aria-label="Wednesday").section__day.Heading Wednesday
                        i.arrow-down
                    .section__desc.desc(role="contentinfo")
                        each item in dayW
