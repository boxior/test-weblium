    menu(type="toolbar" role="menu").navigation.container
        li(role="menuitem").navigation__item
            button(type="button" data-class="state-0-all" aria-label="State of page 1").navigation__btn.active-state State 1
        li(role="menuitem").navigation__item
            button(type="button" data-class="state-0-all-link" aria-label="State of page 2").navigation__btn State 2
